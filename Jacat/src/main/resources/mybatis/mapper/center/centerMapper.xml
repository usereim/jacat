<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="centerMapper">
	<select id="selectLicenseTestCenterByKeyword" parameterType="String"
		resultType="center">
		select * 
		from license_test_center
		where address LIKE CONCAT('%', #{keyword}, '%')
		or exam_area_nm LIKE CONCAT('%', #{keyword}, '%')
		limit 10
	</select>
	
	<select id="selectLicenseTestCenterOne" parameterType="int"
		resultType="center">
		select *
		from license_test_center
		where addno = #{addno}
	</select>
</mapper>